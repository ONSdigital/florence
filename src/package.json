{
  "name": "florence",
  "description": "Florence javascript build",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ONSdigital/florence"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.2.1",
    "babel-jest": "^19.0.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^19.0.2",
    "jest-fetch-mock": "^1.2.0",
    "prop-types": "^15.5.8",
    "react-test-renderer": "^15.5.4",
    "regenerator-runtime": "^0.10.5",
    "source-map-loader": "^0.2.1",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "watch": "cd legacy && npm run watch & npm run watch-refactored",
    "watch-refactored": "node ./node_modules/webpack/bin/webpack.js -d --watch",
    "build": "cd legacy && npm run build && cd ../ && npm run build-refactored",
    "build-refactored": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js -p",
    "build-refactored:dev": "node ./node_modules/webpack/bin/webpack.js -d",
    "test": "export NODE_ENV=testing && jest --no-cache",
    "test-watch": "export NODE_ENV=testing && jest --watch --no-cache",
    "postinstall": "npm run build-refactored"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "backoff": "^2.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.10.0",
    "install": "^0.8.7",
    "node-sass": "^4.5.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "uuid": "^3.0.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
