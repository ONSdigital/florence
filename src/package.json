{
  "description": "Florence javascript build",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ONSdigital/florence"
  },
  "comment": "The handsontable and replace-in-file devDependencies, plus the 'node tablebuilder/handsontable-css-fix.js' build script are only necessary until version 0.3.2+ of react-handsontable is available and pulled in by dp-table-builder-ui",
  "devDependencies": {
    "cat": "latest",
    "onchange": "latest",
    "handlebars": "~3.0.1",
    "handsontable": "^0.36",
    "node-sass": "latest",
    "replace-in-file": "^3.1.1"
  },
  "scripts": {
    "watch": "cd legacy && npm run watch & npm run watch-refactored",
    "watch-refactored": "node ./node_modules/webpack/bin/webpack.js -d --watch",
    "build": "node tablebuilder/handsontable-css-fix.js && cd legacy && npm run build && cd ../ && npm run build-refactored",
    "build-refactored": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js -p",
    "build-refactored:dev": "node ./node_modules/webpack/bin/webpack.js -d",
    "postinstall": "npm run build-refactored"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "dp-table-builder-ui": "^1.0.9",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.10.0",
    "install": "^0.8.7",
    "npm": "^4.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
