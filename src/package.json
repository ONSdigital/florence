{
  "name": "florence",
  "description": "Florence javascript build",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ONSdigital/florence"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "setupFilesAfterEnv": [
      "./setupJestAfterEnv.js"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "collectCoverage": false,
    "globals": {
      "window": true
    },
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "../node_modules/(?!(dis-authorisation-client-js)/)"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.26.10",
    "@babel/node": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.2",
    "babel-regenerator-runtime": "^6.5.0",
    "better-npm-audit": "^1.5.0",
    "cheerio": "^1.0.0-rc.3",
    "clsx": "^1.1.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.7.1",
    "dev-tools": "^0.1.6",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-webpack-plugin": "^3.0.1",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "handsontable": "^0.35.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-now": "^1.3.0",
    "jest-snapshot": "^27.5.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.1",
    "npm-force-resolutions": "0.0.10",
    "prettier": "^2.4.1",
    "prop-types": "^15.5.8",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.10.5",
    "replace-in-file": "^6.3.5",
    "request": "^2.88.2",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.2.1",
    "url": "^0.11.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "comment": "The handsontable and replace-in-file devDependencies, plus the 'node tablebuilder/handsontable-css-fix.js' build & postinstall scripts are only necessary until version 0.3.2+ of react-handsontable is available and pulled in by dp-table-builder-ui",
  "scripts": {
    "audit": "npx auditjs ossi --whitelist ./audit-allowlist.json --quiet",
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "node tablebuilder/handsontable-css-fix.js && cd legacy && npm run build && cd ../ && npm run build-refactored",
    "watch": "cd legacy && npm run watch & npm run watch-refactored",
    "watch-refactored": "export NODE_ENV=development && npx webpack -w & npm run prettier-watch",
    "legacy:watch": "cd legacy && npm run watch",
    "build-refactored": "npm run prettier-cli; node ./node_modules/webpack/bin/webpack.js --mode=production",
    "build-refactored:dev": "node ./node_modules/webpack/bin/webpack.js -d",
    "test": "export NODE_ENV=testing TZ='Europe/London' && jest",
    "test-watch": "export NODE_ENV=testing  TZ='Europe/London' && ./node_modules/jest/bin/jest.js --watch --no-cache",
    "test:file": "export NODE_ENV=testing TZ='Europe/London' && jest $npm_config_file",
    "test:update": "export NODE_ENV=testing && jest --updateSnapshot",
    "preinstall": "npx npm-force-resolutions && mkdir -p ../dist && cp -R img/ ../dist/img/",
    "postinstall": "node tablebuilder/handsontable-css-fix.js && npm run build-refactored",
    "prettier-test": "prettier --config \"prettierrc.json\" --check \"app/**/*.{js,jsx}\"",
    "prettier-cli": "prettier --config \"prettierrc.json\" --write \"app/**/*.{js,jsx}\"",
    "prettier-watch": "onchange \"app/**/*.{js,jsx}\" -- prettier --config \"prettierrc.json\" --write {{changed}}",
    "eslint-check": "npx --yes eslint@8.44.0 -c ./.eslintrc.json ."
  },
  "dependencies": {
    "acorn": "^8.4.1",
    "auditjs": "^4.0.37",
    "dateformat": "^2.0.0",
    "dis-authorisation-client-js": "^1.0.0",
    "dp-table-builder-ui": "^1.1.2",
    "is-empty-object": "^1.1.1",
    "json5": "^2.2.2",
    "localforage": "^1.5.0",
    "minimongo": "^6.15.1",
    "moment": "^2.29.1",
    "node-fetch": "^3.3.1",
    "onchange": "^6.1.0",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^3.2.6",
    "react-router-redux": "^4.0.8",
    "react-select": "^5.10.1",
    "react-table": "^7.7.0",
    "react-transition-group": "^2.9.0",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.3",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.1.4",
    "sass": "^1.34.1",
    "ssri": "^6.0.2",
    "uuid": "^3.4.0"
  },
  "overrides": {
    "path-to-regexp": ">=8.0.0",
    "semver": ">=7.5.4",
    "cross-spawn": ">=7.0.5",
    "@babel/runtime": ">=7.26.10",
    "brace-expansion": "1.1.12"
  }
}
