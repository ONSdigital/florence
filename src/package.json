{
  "description": "Florence javascript build",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ONSdigital/florence"
  },
  "devDependencies": {
    "cat": "latest",
    "onchange": "latest",
    "handlebars": "~3.0.1",
    "node-sass": "latest"
  },
  "scripts": {
    "watch-css": "npm run build-css && ./node_modules/onchange/cli.js 'scss/*.scss' 'scss/*/*.scss' -v -- npm run build-css",
    "build-css": "./node_modules/node-sass/bin/node-sass --output-style expanded --source-map assets/css scss/main.scss assets/css/main.min.css",
    "watch": "node ./node_modules/webpack/bin/webpack.js --watch",
    "postinstall": "npm run build",
    "build": "node ./node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
