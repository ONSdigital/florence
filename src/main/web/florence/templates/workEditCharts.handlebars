<div class="edit-section">
  <div class="edit-section__head">
    <h1>Charts</h1>

    <p>Title</p>
  </div>
  <div class="edit-section__content">
    <div id="chart-list" class="edit-section__sortable">
        {{#each this.charts}}
          <div id="chart_{{filename}}" class="edit-section__sortable-item">
              {{title}}
            <button class="btn-markdown-edit" id="chart-edit_{{filename}}">Edit</button>
            <button class="btn-page-delete" id="chart-delete_{{filename}}">Delete</button>
          </div>
        {{/each}}
    </div>
  </div>
</div>